<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<!DOCTYPE html>
<html>
<head>
<title>아이디 찾기</title>
</head>
<body>
	<form>
		<table>
			<caption>일반 아이디 찾기</caption>
			<tr>
				<td>이름</td>
				<td><input type="text" id="name" name="name" placeholder="이름" required></td>
			</tr>
			
			<tr>
				<td>핸드폰</td>
				<td><input type="number" id="number" name="number" placeholder="핸드폰" required></td>
			</tr>
		</table>
		 <input type="submit" value="인증완료"><br>
		 
		<table>
			<caption>기업 아이디 찾기</caption>
			<tr>
				<td>담당자 명</td>
				<td><input type="text" id="name" name="name" placeholder="담당자 명" required></td>
			</tr>
			
			<tr>
				<td>핸드폰</td>
				<td><input type="number" id="number" name="number" placeholder="핸드폰" required></td>
			</tr>
		</table>
		 <input type="submit" value="인증완료"><br>
	</form>
</body>
</html>